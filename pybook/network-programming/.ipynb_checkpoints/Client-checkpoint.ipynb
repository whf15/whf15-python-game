   "source": [
    "import socket\n",
    "host = '127.0.0.1'\n",
    "port = 8080\n",
    "s = socket.socket()\n",
    "s.connect((host,port))\n",
    "send_data = input(\"请输入要发送的数据：\")\n",
    "s.send(send_data.encode())\n",
    "# 接受对方发送过来的数据，最大接受1024个字节\n",
    "recvData = s.recv(1024).decode()\n",
    "print(\"接收到的数据为：\",recvData)\n",
    "#关闭套接字\n",
    "s.close()"
   ]
